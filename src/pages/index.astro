---
// Component imports
import Nav from "../components/Nav.astro";
import Layout from "../layouts/layout.astro";

// Image imports
import roshan from "../images/content/roshan.png";
import team from "../images/content/team.jpg";
---

<!-- Imports -->
<Layout />

<Nav />
<!-- Website body -->

<div class="h-screen w-full relative max-sm:absolute">
  <img src={team}  alt="" class="w-full  h-1/3 max-sm:h-1/2 object-cover  absolute bg-fixed brightness-50   "> 
  
  <div class="absolute w-full mt-20 place-self-center">
    <h1 class="text-4xl  font-extrabold text-white ml-20 ">Our Team</h1>

    <p class="text-2xl ml-20 mt-5 mr-10 font-roboto-slab text-white"> Our team consists of very diverse members, with a warm and inclusive communiy. We strive to inspire our membbers to do their best and nurture their passion for STEM. We compete in FIRST robotics competitions.</p> 
  </div>

  
</div>

